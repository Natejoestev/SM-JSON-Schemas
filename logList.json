{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/Tofixrs/SM-JSON-Schemas/master/logList.json",
  "title": "logList",
  "description": "",
  "type": "object",
  "properties": {
    "logList": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "uuid": {
            "type": "string",
            "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
          },
          "type": { "type": "string" },
          "photo": { "type": "string" },
          "location": { "type": "string" },
          "icon": { "type": "string" }
        }
      }
    }
  }
}
