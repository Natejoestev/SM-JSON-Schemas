{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/Tofixrs/SM-JSON-Schemas/master/rend.json",
  "title": "Vec3",
  "description": "",
  "type": "object",
  "properties": {
    "lodList": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "maxViewDistance": { "type": "integer" },
          "mesh": { "type": "string" },
          "includes": {
            "type": "array",
            "items": {
              "enum": ["POSITION", "NORMAL", "UV0", "VERTEX_COLOR", "TANGENT"]
            }
          },
          "subMeshMap": {
            "patternProperties": {
              "^.*$": {
                "material": { "enum": ["DifAsgNorAo"] },
                "textureList": {
                  "type": "array",
                  "items": { "type": "string" }
                }
              }
            }
          }
        }
      }
    }
  }
}
