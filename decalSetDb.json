{
  "$id": "./decalSetDb.json",
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "decalSetDb",
  "description": "List of Decal Sets",
  "type": "object",
  "properties": {
    "decalSetList": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "set": {
            "$ref": "./smPath.json"
          },
          "category": {
            "type": "string"
          }
        },
        "required": [
          "set"
        ]
      }
    }
  },
  "required": [
    "decalSetList"
  ]
}