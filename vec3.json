{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "./vec3.json",
  "title": "Vec3",
  "description": "An Object representing a 3D vector. {x:X, y:Y, z:Z}",
  "additionalProperties": false,
  "type": "object",
  "properties": {
    "x": {
      "type": "number"
    },
    "y": {
      "type": "number"
    },
    "z": {
      "type": "number"
    }
  },
  "required": [
    "x",
    "y",
    "z"
  ]
}